{
  "name": "schema-first",
  "version": "0.1.7",
  "description": "schema first model",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drnachio/schema-first.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Ignacio Ferro Pic√≥n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drnachio/schema-first/issues"
  },
  "homepage": "https://github.com/drnachio/schema-first#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
